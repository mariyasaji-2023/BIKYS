<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script>
    const removeButtons = document.querySelectorAll('.remove-product');

    removeButtons.forEach((button) => {
        button.addEventListener('click', function (event) {
            event.preventDefault();

            const productId = button.getAttribute('data-product-id');

            Swal.fire({
                title: 'Are you sure?',
                text: 'You won\'t be able to revert this!',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, remove it!',
            }).then((result) => {
                if (result.isConfirmed) {

                    fetch(`/wishlist/remove/${productId}`, {
                            method: 'POST',
                        })
                        .then((response) => response.json())
                        .then((data) => {
                            if (data.message ===
                                'Product removed from wishlist successfully') {
                                // Display a success message
                                Swal.fire({
                                    title: 'Deleted!',
                                    text: 'Product removed from wishlist successfully.',
                                    icon: 'success',
                                    timer: 1500, // Set the timer to 1.5 seconds
                                    showConfirmButton: false, // Hide the "OK" button
                                }).then(() => {
                                    window.location.reload()
                                })

                            } else {
                                // Handle any other error or message here
                                // You can display an error message in a similar way
                                Swal.fire('Error', 'Failed to remove product from wishlist',
                                    'error');
                            }
                        })
                        .catch((error) => {
                            console.error(error);
                            // Handle the error here
                            Swal.fire('Error', 'An error occurred', 'error');
                        });
                }
            });
        });
    });



    const addToCartButtons = document.querySelectorAll('.add-to-cart');

    addToCartButtons.forEach((button) => {
        button.addEventListener('click', function (event) {
            event.preventDefault();

            const productId = button.getAttribute('data-product-id');
            console.log('ProductId:', productId);
            alert(productId)
            // Make an AJAX request to move the product from the wishlist to the cart
            fetch(`/wishlist/addtocart/${productId}`, {
                    method: 'POST',
                })
                .then((response) => response.json())
                .then((data) => {
                    alert(data.message)
                    alert(data,"addtocart data");
                    if (data.message === 'Product moved from wishlist to cart successfully') {
                        Swal.fire({
                            icon: 'success',
                            title: 'Success',
                            text: 'Product moved to cart successfully',
                            timer: 1500, // Set the timer to 1500 milliseconds (1.5 seconds)
                            showConfirmButton: false,
                        }).then(()=>{
                            window.location.reload()
                        })
                            
                    } else {
                        Swal.fire({
                            icon: 'error',
                            title: 'Error',
                            text: 'Failed to move product to cart',
                        });
                    }
                })
                .catch((error) => {
                    console.error(error);
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: 'An error occurred',
                    });
                });
        });
    });
</script>


